journal = {Psychonomic Bulletin {\&} Review},
keywords = {Executive Function,Eye Movements,Humans,Language,Language Tests,Photic Stimulation,Reaction Time,Stroop Test,Task Performance and Analysis,Verbal Behavior,Visual Perception},
month = {oct},
