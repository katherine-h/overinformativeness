@article{Bock1986,
abstract = {Used a syntactic priming procedure with 48 undergraduates to examine the implications for theories of language performance of the claim that activation processes have an impact on the mechanisms of language use, including those responsible for syntactic structure in speech. On each priming trial, Ss produced a priming sentence in 1 of several syntactic forms. They then viewed an unrelated event in a picture and described it in 1 sentence. Findings show that the probability of a particular syntactic form being used in the description increased when that form had occurred in the prime, under presentation conditions that minimized Ss' attention to their speech, to the syntactic features of the priming sentences, and to connections between the priming sentences and the subsequent pictures. This syntactic repetition effect suggests that sentence formulation processes are somewhat inertial and subject to such probabilistic factors as the frequency or recency of use of particular structural forms. Exps II and III, each with 48 undergraduates, showed that this effect was not appreciably modified by variations in conceptual characteristics of sentences. Overall results reveal that the effects of priming were specific to features of sentence form, independent of sentence content. The empirical isolability of structural features from conceptual characteristics of successive utterances is consistent with the assumption that some syntactic processes are organized into a functionally independent subsystem. Priming sentences and test instructions for Exp I are appended. (49 ref) (PsycINFO Database Record (c) 2010 APA, all rights reserved)},
author = {Bock, J Kathryn},
doi = {10.1016/0010-0285(86)90004-6},
issn = {00100285},
journal = {Cognitive Psychology},
number = {3},
pages = {355--387},
publisher = {Elsevier},
title = {{Syntactic persistence in language production}},
url = {http://linkinghub.elsevier.com/retrieve/pii/0010028586900046},
volume = {18},
year = {1986}
}
